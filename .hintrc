{
  "extends": [
    "development"
  ],
  "hints": {
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "width: fit-content",
          "mask-position"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 122",
    "not chrome <= 122",
    "not edge <= 122",
    "not opera <= 106",
    "not samsung <= 23"
  ]
}